{
  "manifest_version": 2,
  "name": "NSA Girlfriend",
  "version": "1.0",
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["jquery.min.js", "main.js"]
  }
],
 "permissions": [
   "activeTab",
   "tabs",
   "notifications",
   "https://ajax.googleapis.com/",
   "webRequest",
   "background"
 ]
}
